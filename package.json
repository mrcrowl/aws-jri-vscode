{
  "name": "aws-jri",
  "displayName": "aws-jri",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.67.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:jri.route53HostedZones",
    "onCommand:jri.ecsClusters",
    "onCommand:jri.s3Buckets",
    "onCommand:jri.rdsDatabases",
    "onCommand:jri.lambdaFunctions"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "jri.route53HostedZones",
        "title": "jri: Route 53 — Hosted Zones"
      },
      {
        "command": "jri.ecsClusters",
        "title": "jri: ECS — Clusters"
      },
      {
        "command": "jri.s3Buckets",
        "title": "jri: S3 — Buckets"
      },
      {
        "command": "jri.lambdaFunctions",
        "title": "jri: Lambda — Functions"
      },
      {
        "command": "jri.rdsDatabases",
        "title": "jri: RDS — Databases"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "@aws-sdk/client-cloudfront": "3.92.0",
    "@aws-sdk/client-route-53": "3.92.0",
    "@aws-sdk/client-s3": "3.92.0",
    "@aws-sdk/client-lambda": "3.92.0",
    "@aws-sdk/client-ecs": "3.92.0",
    "@aws-sdk/client-rds": "3.92.0"
  },
  "devDependencies": {
    "@types/vscode": "^1.67.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.9.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.1",
    "typescript": "^4.5.5",
    "ts-loader": "^9.2.6",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "@vscode/test-electron": "^2.1.2"
  }
}
